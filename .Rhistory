# Rounds to nearest 2
roundcust <- function(x){
ifelse(0 < x & x <= 2, 1,
ifelse(2<x& x <=4, 3,
ifelse(4<x& x <=6, 5,
ifelse(6<x& x <=8, 7,
ifelse(8<x& x <=10, 9,
ifelse(10<x& x <=12, 11,
ifelse(12<x& x <=14, 13,0)
))))))}
